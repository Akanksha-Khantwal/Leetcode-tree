void inorder(Node* root,vector<int>& ans){
    if(!root)
    return;
    inorder(root->left,ans);
    ans.push_back(root->data);
    inorder(root->right,ans);
}
int sum(Node* root, int k) {
    vector<int>ans;
    inorder(root,ans);
    if(k>ans.size())
    return -1;
    int sum=0;
    for(int i=0;i<k;i++){
        sum+=ans[i];
    }
    return sum;
}
